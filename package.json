{
  "name": "saga-feature-flow",
  "version": "0.0.0",
  "description": "A login/register flow built with React & Redux Saga",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack/prod.js --progress --colors -p",
    "test": "standard | snazzy --verbose && NODE_ENV=test ava --verbose"
  },
  "author": "LaunchDarkly <team@launchdarkly.com>",
  "repository": "atrakh/saga-feature-flow",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "ava": "0.17.0",
    "babel-core": "6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "css-loader": "0.26.1",
    "html-webpack-plugin": "^2.26.0",
    "localStorage": "1.0.3",
    "postcss-focus": "1.0.0",
    "postcss-import": "9.0.0",
    "postcss-loader": "1.2.1",
    "postcss-reporter": "3.0.0",
    "postcss-simple-vars": "3.0.0",
    "redux-ava": "2.2.0",
    "snazzy": "5.0.0",
    "standard": "8.6.0",
    "style-loader": "0.13.1",
    "surge": "0.18.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "bcryptjs": "2.4.0",
    "btoa": "1.1.2",
    "ldclient-js": "^1.1.6",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "2.5.1",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-saga": "0.14.2",
    "webpack": "^1.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
